{
  "manifest_version": 2,
  "name": "GitHub Project Tweaks",
  "version": "0.2.0",
  "description": "Adds WiP limits to GitHub project boards",
  "icons": {
    "48": "icon_48.png",
    "96": "icon_96.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.github.com/orgs/*/projects/*",
        "*://*.github.com/orgs/*/projects/*/views/*"
      ],
      "js": ["github_project_tweaks.js"]
    }
  ]
}
