{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["standard", "plugin:jsdoc/recommended-error"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": ["error", "always"],
    "semi-style": ["error", "last"],
    "space-before-function-paren": ["error", "never"]
  },
  "plugins": [
    "jsdoc"
  ]
}
